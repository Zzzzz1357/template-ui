{
    "printWidth": 120,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "overrides": [
        {
            "files": "*.html",
            "options": {
                "parser":"html"
            }
        },
        {
            "files":["*.prettierrc", "*.json"],
            "options": {
                "parser":"json"
            }
        }
    ]
}